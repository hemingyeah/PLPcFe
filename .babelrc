{
  "presets": [
    ["env", { 
      "modules": false,
      "targets": {
        "browsers": ["> 1%","last 2 versions","ie >= 10"]
      }
    }]
  ],
  "plugins": [
    "lodash",
    "syntax-dynamic-import",
    "transform-runtime",
    "transform-object-rest-spread",
    "transform-async-to-generator",
    "transform-decorators-legacy",
    "transform-decorators",
    "transform-vue-jsx"
  ]
}